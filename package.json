{
  "name": "douyin-pull",
  "version": "1.0.0",
  "description": "拉取抖音粉丝数的一个小工具",
  "main": "./out/main/index.js",
  "author": "成雨",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "env": "cross-env ELECTRON_MIRROR=\"https://npmmirror.com/mirrors/electron/\" ELECTRON_BUILDER_BINARIES_MIRROR=\"https://npmmirror.com/mirrors/electron-builder-binaries/\"",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "pnpm run typecheck && electron-vite build",
    "preinstall": "pnpm add cross-env -D",
    "postinstall": "pnpm run env electron-builder install-app-deps",
    "build:win:env": "pnpm run env pnpm run build:win",
    "build:win": "cross-env-shell \"pnpm run build && electron-builder --win --config\"",
    "build:mac:env": "pnpm run env pnpm run build:mac",
    "build:mac": "cross-env-shell \"electron-vite build && electron-builder --mac --config\"",
    "build:linux:env": "pnpm run env pnpm run build:linux",
    "build:linux": "cross-env-shell \"electron-vite build && electron-builder --linux --config\""
  },
  "dependencies": {
    "@electron-toolkit/preload": "2.0.0",
    "@electron-toolkit/utils": "2.0.0",
    "music-metadata": "7.14.0",
    "nedb-promises": "6.2.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "1.0.1",
    "@electron-toolkit/eslint-config-ts": "1.0.0",
    "@electron-toolkit/tsconfig": "1.0.1",
    "@types/howler": "2.2.11",
    "@types/node": "18.17.5",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@vitejs/plugin-react": "4.0.4",
    "ahooks": "3.7.8",
    "autoprefixer": "10.4.16",
    "classnames": "2.3.2",
    "cross-env": "7.0.3",
    "daisyui": "4.4.10",
    "electron": "25.6.0",
    "electron-builder": "24.6.3",
    "electron-vite": "1.0.27",
    "eslint": "8.47.0",
    "eslint-plugin-react": "7.33.2",
    "lodash-es": "4.17.21",
    "postcss": "8.4.31",
    "postcss-nesting": "12.0.1",
    "prettier": "3.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.20.1",
    "tailwindcss": "3.3.5",
    "typescript": "5.1.6",
    "valtio": "1.12.1",
    "vite": "4.4.9"
  },
  "packageManager": "pnpm@7.12.0"
}
